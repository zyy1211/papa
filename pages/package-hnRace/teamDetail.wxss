.wxapp-modal {
    position: fixed;
    width: 100%;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    z-index: 999;
}

.modal-header {
    padding-top: 20rpx;
}

.modal-content .modal-body {
    padding: 30rpx;
    font-size: 28rpx;
    color: #adb2bb;
}

.wxapp-modal .content {
    position: absolute;
    top: 25%;
    left: 50%;
    z-index: 1000;
    width: 80%;
    margin: 0 0 0 -40%;
    background: #fff;
    text-align: center;
    border-radius: 10rpx;
}

.wxapp-modal .content .header {
    margin: auto;
    width: 100%;
    height: 100rpx;
    font-weight: bold;
    line-height: 100rpx;
    text-align: center;
}

.wxapp-modal .content .body {
    margin: auto;
    width: 100%;
    padding: 0 0 40rpx 0;
}

.wxapp-modal .content .footer {
    margin: auto;
    width: 100%;
    height: 80rpx;
    line-height: 80rpx;
    border-top: 1px #eee solid;
}

.wxapp-modal .content .footer button {
    display: inline-block;
    width: 49%;
    height: 80rpx;
    line-height: 80rpx;
    background: #fff;
}

.wxapp-modal .content .footer button::after {
    content: none;
    border: none;
}

.wxapp-modal .content .footer .cancel {
    color: #000;
    border-right: 1px #eee solid;
}

.wxapp-modal .content .footer .confirm {
    color: #00CD91;
}

.wxapp-modal .mask {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background-color: rgba(0,0,0,0.5);
}

