.fix-ipx-content {
    position: fixed;
    bottom: 0;
    width: 100%;
    height: 68rpx;
    background: #fff;
    z-index: 1001;
}

.data-list-con {
    box-sizing: border-box;
    width: 100%;
    margin-top: 26rpx;
    background: #fff;
    padding: 0 27rpx;
    margin-bottom: 0;
}

.data-list-con.has-ipx {
    margin-bottom: 68rpx;
}

.data-item-con {
    box-sizing: border-box;
    width: 100%;
    padding: 30rpx 14rpx;
    border-bottom: 2rpx solid #F3F3F3;
}

.data-item-con:last-child {
    border: none;
}

.no-fans-con {
    margin-top: 226rpx;
    width: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    background: transparent;
}

.no-data-img {
    width: 277rpx;
    height: 165rpx;
    margin-top: 150rpx;
}

.no-data-text {
    margin-top: 35rpx;
    font-size: 30rpx;
    color: #666;
}